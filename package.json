{
  "name": "unit-converter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite src",
    "build": "node build/build_credits.cjs && tsc && vite build src --emptyOutDir",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "npm run build && cd dist && npx http-server -o /unit-converter/",
    "deploy": "npx gh-pages -d dist/unit-converter"
  },
  "dependencies": {
    "@types/node": "^20.3.1",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.5",
    "convertible-to-bigint": "github:nscardina/convertible-to-bigint",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-dom": "^18.2.0",
    "ts-fraction": "github:nscardina/ts-fraction"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.2.10",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "gh-pages": "^3.0.0",
    "http-server": "^14.1.1",
    "typescript": "^5.0.2",
    "vite": "^5.0.11",
    "vite-plugin-ejs": "^1.6.4"
  }
}
